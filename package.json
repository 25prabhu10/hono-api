{
  "name": "hono-api",
  "type": "module",
  "version": "0.0.1",
  "description": "Template for building APIs with Hono",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/25prabhu10/hono-api.git"
  },
  "keywords": [
    "hono",
    "api",
    "typescript"
  ],
  "main": "src/index.ts",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "tsx watch --env-file=.env src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node ./dist/src/index.js",
    "lint": "eslint --cache .",
    "lint:fix": "npm run lint --fix",
    "typecheck": "tsc --pretty --noEmit",
    "test": "cross-env NODE_ENV=test vitest"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.8",
    "@hono/zod-openapi": "^0.19.2",
    "@libsql/client": "^0.14.0",
    "@scalar/hono-api-reference": "^0.7.2",
    "drizzle-orm": "^0.40.1",
    "drizzle-zod": "^0.7.0",
    "hono": "^4.7.5",
    "hono-pino": "^0.8.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.10.1",
    "@types/node": "^22.13.10",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.19.13",
    "eslint": "^9.22.0",
    "tsc-alias": "^1.8.11",
    "tsx": "^4.19.3",
    "vitest": "^3.0.9"
  }
}
